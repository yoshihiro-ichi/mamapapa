<%= form_with model: @user, url:user_path do |f| %>
  <div class="field">
  <%= f.label :image %>
   <%= image_tag @user.image.url if @user.image && @user.image.url %>
   <%= f.file_field :image %>
   <%= f.hidden_field :image_cache %>
   <%= f.label :name %>
   <%= f.text_field :name %>
   <%= f.label :email %>
   <%= f.email_field :email %>
   <%= f.label :password %>
   <%= f.password_field :password %>
   <%= f.label :password_confirmation %>
   <%= f.password_field :password_confirmation %>
   <%= f.submit "プロフィール編集完了" %>
<%end%>
